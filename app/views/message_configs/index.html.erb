<h1>Listing message_configs</h1>

<table>
  <thead>
    <tr>
      <th>Group</th>
      <th>Biztype</th>
      <th>Name</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @message_configs.each do |message_config| %>
      <tr>
        <td><%= message_config.group %></td>
        <td><%= message_config.bizType %></td>
        <td><%= message_config.name %></td>
        <td><%= message_config.desc %></td>
        <td><%= link_to 'Show', message_config %></td>
        <td><%= link_to 'Edit', edit_message_config_path(message_config) %></td>
        <td><%= link_to 'Destroy', message_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message config', new_message_config_path %>
